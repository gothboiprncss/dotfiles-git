{
    "hyprland/workspaces": {
        "format": "{icon}",
        "format-icons": {
            "1": "I",
            "2": "II",
            "3": "III",
            "4": "IV",
            "5": "V",
            "6": "VI",
            "7": "VII",
            "8": "VIII",
            "9": "IX",
            "10": "X"
        },
        "on-click": "activate",
        "sort-by-number": true,
        "active-only": false
    },
    "hyprland/window": {
        "format": "<span foreground=\"#A7C080\">üêà</span> {}",
        "max-length": 100
    },
    "clock": {
        "format": "({:%H~%M})",
        "format-alt": "/{:%d %b %Y = %H:%M}/"
    },
    "backlight": {
        "device": "intel_backlight",
        "format": "<span foreground=\"#A7C080\">{icon}</span> {percent}% ",
        "format-icons": [
            "ÔÜÖ",
            "ÔÑë"
        ],
        "on-click": "pkill -USR1 gammastep || gammastep",
        "tooltip-format": "Brightness"
    },
    "pulseaudio": {
        "format": "<span foreground=\"#3D484D\"></span><span foreground=\"#A7C080\">{icon}</span> {volume}% ",
        "format-bluetooth": "<span foreground=\"#3D484D\"></span> <span foreground=\"#A7C080\">Ôäî</span> {volume}% ",
        "format-muted": "<span foreground=\"#3D484D\"></span> <span foreground=\"#A7C080\">ÔÄ¶</span> ",
        "format-icons": {
            "headphone": "ÔÄ•",
            "default": [
                "ÔÄß",
                "ÔÄ®"
            ]
        },
        "scroll-step": 1,
        "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
        "ignored-sinks": [
            "Easy Effects Sink"
        ]
    },
    "network": {
        "format": "<span foreground=\"#3D484D\">|</span> {ifname}",
        "format-wifi": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\">Ôá´</span> {signalStrength}%",
        "format-ethernet": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\">Û∞äó</span> {ipaddr}/{cidr}",
        "format-disconnected": "",
        "tooltip-format": "{ifname} via {gwaddr}",
        "tooltip-format-wifi": "{essid} ({signalStrength}%)",
        "tooltip-format-ethernet": "{ifname}",
        "tooltip-format-disconnected": "Disconnected",
        "max-length": 50
    },
    "hyprland/language": {
        "format": "<span foreground=\"#A7C080\">Û∞åå</span> {short} <span foreground=\"#3D484D\">|</span>"
    },
    "disk": {
        "interval": 30,
        "format": "<span foreground=\"#A7C080\" weight=\"bold\">/</span> {percentage_used}% ",
        "path": "/"
    },
    "cpu": {
        "format": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\" weight=\"bold\">CPU</span> {usage}% "
    },
    "memory": {
        "format": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\" weight=\"bold\">RAM</span> {}%"
    },
    "custom/wlogout": {
        "format": "(>·¥ó‚Ä¢)",  
        "on-click": "wlogout",
        "tooltip": false
    }
}
